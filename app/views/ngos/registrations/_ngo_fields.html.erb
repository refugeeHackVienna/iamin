<h4><i class="fas fa-users" aria-hidden="true"></i> <%= t('.profile') %></h4>
<%= f.input :name, autofocus: true %>
<%= f.input :email %>
<%= f.input :password, autocomplete: 'off', hint: "(#{@minimum_password_length} #{t('.min_characters')})" %>
<%= f.input :password_confirmation, autocomplete: 'off' %>
